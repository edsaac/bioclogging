FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     filtrationEd;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime			86400;
//endTime         172800;
//endTime         259200;

//deltaT          0.5;
deltaT       10;

writeControl    runTime;

writeInterval   1800;
//writeInterval   3600;

purgeWrite      0;

writeFormat     ascii;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           1.0;

